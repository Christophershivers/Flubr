<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link rel="stylesheet" href="styles.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
        crossorigin="anonymous"></script>


    <script src="https://kit.fontawesome.com/b35c1022ab.js" crossorigin="anonymous"></script>


</head>

<body>
    <div class="container text-center" style="padding-top: 40px;">
        <div class="row align-items-start">
            <div class="col-3">
                
            </div>
            <div class="col-6" align="left" style=" text-align: left; ">

                <div class="start" >
                    <div class="container" style=" padding-bottom: 70px; padding-left: 0px; background-color: #2980b9; box-shadow: 10px -2px 41px -13px rgba(0,0,0,0.49);-webkit-box-shadow: 10px -2px 41px -13px rgba(0,0,0,0.49); -moz-box-shadow: 10px -2px 41px -13px rgba(0,0,0,0.49);">
                        <h2 style="text-align: center; position: relative; top: 35px;">Search for Friends</h2>
                    </div>
                    
                    @foreach(var item in Model){
                    
                            <div class="people" style="border: 1px solid rgb(216, 216, 216); padding-bottom: 20px; background-color: #ffffff; ">
                                <img src="@item.ProfileImage" alt="" onerror="this.onerror=null" this.src="logo.png" width="100" height="90" style="border-radius: 50%; float: left;">
                                <p style="float: left;">@item.FirstName @item.LastName</p><p style="float: left;"></P><p style="float: none;">&nbsp; @item.UserName</p>
                                <a asp-controller="Profile" asp-action="Account" asp-route-id="@item.Id"><button type="button" class="btn btn-primary"  style="position:relative; bottom: 10px; left: 5px;">View Page</button></a>
                            </div>
                    
                    
                    
                    
                    
                    }
                   
                    
                </div>
               
            </div>










            <div class="col-3" >
                
                    
                    
            
                
            </div>
        </div>
    </div>

</body>

</html>






