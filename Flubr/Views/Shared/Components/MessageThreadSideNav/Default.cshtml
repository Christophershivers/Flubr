@model List<MessageThreadCascade>


@foreach(var thread in Model){
            <li class="sidenav-item">
                <a asp-controller="Messages" asp-action="Open" asp-route-id="@thread.MessageId" class="list-group-item list-group-item-action d-flex align-items-center sidenav-link" aria-current="true">
                        <!--<img src="@thread.ProfileImage" class="border rounded-circle me-2" alt=""
                          style="height: 40px" />-->
                        <strong>@thread.MessageName</strong>
                </a>
            </li>
    
    }